- Create instance with amzon linux (t2.micro)

- File to create cluster

kind: ClusterConfig
apiVersion: eksctl.io/v1alpha5
# target eksctl version 0.69.0
metadata:
  name: bidclips-parth
  version: "1.21"
  region: ap-southeast-1

# commented below part is advisable to implement only in production clusters to save some $$$
vpc:
  autoAllocateIPv6: true  # this will enable dual stack networking support for load balancers
#   nat:
#     gateway: "HighlyAvailable"  # this will create one NAT gateway (and one EIP) per availability zone

nodeGroups:
  - name: newdev-ng-c5-xl
    instanceType: c5.xlarge
    instancePrefix: aws-mngd-c5-xl
    instanceName: newdev-node
    minSize: 2
    desiredCapacity: 2
    maxSize: 3
    amiFamily: AmazonLinux2
    labels: {role: worker}
    tags:
      nodegroup-role: worker
    privateNetworking: true
    availabilityZones: ["ap-southeast-1a", "ap-southeast-1b", "ap-southeast-1c"]
    iam:
      withAddonPolicies:
        albIngress: true
        autoScaler: true
    updateConfig:
      maxUnavailable: 1
      # maxUnavailablePercentage: 25%
    containerRuntime: containerd
availabilityZones: ["ap-southeast-1a", "ap-southeast-1b", "ap-southeast-1c"]


